import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
    Scanner k=new Scanner(System.in);
        int n,t;
        n=k.nextInt();
        t=k.nextInt();
          int r1=n/t;
    int r2=n%t;
  System.out.printf("The number of students in each team is %d and left out is %d",r1,r2);

    }
}
